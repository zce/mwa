{
  "name": "mwa",
  "version": "1.0.0",
  "description": "Easily create your own middleware layer.",
  "keywords": [
    "mwa",
    "compose",
    "middleware",
    "zce"
  ],
  "repository": "zce/mwa",
  "license": "MIT",
  "author": "zce <w@zce.me> (https://zce.me)",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsup",
    "test": "vitest run"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": "esm",
    "target": "es2022",
    "clean": true,
    "dts": {
      "entry": "src/index.ts"
    }
  },
  "prettier": "@zce/prettier-config",
  "devDependencies": {
    "@zce/prettier-config": "^0.2.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18"
  }
}
